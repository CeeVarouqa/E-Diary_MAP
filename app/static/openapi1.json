            "description": "User login",
            "schema": {
              "type": "string",
              "example": "Alina"
            }
          },
          {
            "in": "query",
            "name": "password",
            "required": true,
            "description": "User password",
            "schema": {
              "type": "string",
              "example": "*****"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "success",
