{
  "openapi": "3.0.0",
  "info": {
    "version": "1.0.0",
    "title": "Ediary API",
    "description": "An API for diary app"
  },
  "paths": {
    "/registration": {
      "post": {
        "tags": [
          "Registrationnnn"
        ],
        "description": "Registration API",
        "parameters": [
          {
            "in": "query",
            "name": "username",
